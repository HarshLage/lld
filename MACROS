#include<bits/stdc++.h>
using namespace std;
#define PI 3.14
#define MAXX(x,y)  (x > y ? x : y) // in preprocessing it removes this statement and replace in code
float areaperimeter(float r){
  return 2 * PI * r;
}
int  compare(int x , int y){
  int a =  MAXX(x,y);
  return a;
}
int main()
{ 
  cout<<areaperimeter(4.5)<<endl;
  cout<<compare(4,5)<<endl;
  


  return 0; 
}
