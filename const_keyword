#include<bits/stdc++.h>
using namespace std;
int x = 34;//global variable 
int main()
{ 
  //CONSTANT DATA NON CONSTANT POINTER 
  // const int * a = new int(2);
  // // int  const *a = new int(2); both are the same above and this line 
  // cout<<*a<<endl;
  // // *a = 3; this cant be worked
  // //anoter way
  // int b = 30;
  // a = & b;
  // //this can be worked
  // cout<<*a<<endl;

  //NON CONSTANT DATA CONSTANT POINTER
   int *const a = new int(2);
  cout<<*a<<endl;
  *a = 35;
  cout<<*a<<endl;

  //CONSTANT DATA CONSTANT POINTER

  const int *const ab = new int(2);
  cout<<*ab<<endl;
  // *ab = 5;throws an error because in this case no one should not be changed


}
